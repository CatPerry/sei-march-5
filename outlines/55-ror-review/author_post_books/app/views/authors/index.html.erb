<h1>Authors#index</h1>
<p>Find me in app/views/authors/index.html.erb</p>

<ul>
<% @authors.each do |author| %>
  <li>
    <strong>Name:</strong> <%= author.name %>

    <%= link_to "Edit", edit_author_path(author.id) %>
    <%= link_to "Show", author_path(author.id) %>
    <%= link_to "Delete", author_path(author.id), method: :delete %>
  </li>
<% end %>
</ul>