<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>

<ul>
<% @posts.each do |post| %>
  <li>
    <strong><%= post.author.name %></strong><br />
    <em><%= post.title %></em><br />
    <%= post.content %><br />

    <%= link_to "Edit", edit_post_path(post.id) %>
    <%= link_to "Show", post_path(post.id) %>
    <%= link_to "Delete", post_path(post.id), method: :delete %>
  </li>
<% end %>
</ul>